РАБОТА:=№1/1 - Кинематика точки (плоский случай);
ВЫПОЛНИЛ:=ст.Иванов гр.И-28а;
      # Исходные данные: #
   x =3*sin(pi*t/3);  y=-2*cos(pi*t/3);
      # Решение: #
# Проекции скорости т.М на координатные оси: #
vx=x't; vy=y't;
# Проекции ускорения т.М на оси координат: #
ax=vx't; ay=vy't;
# Кинематические параметры: #
v=sqrt(vx^2+vy^2);                   #  скорость т.М #
a=sqrt(ax^2+ay^2);                   # ускорение т.М #
atau=(vx*ax+vy*ay)/v; # тангенциальное ускорение т.М #
an=sqrt(a^2-atau^2);  #     нормальное ускорение т.М #
ro=v^2/an;            #   радиус кривизны траектории #
     # Инструкции #
ВАРЬИРОВАТЬ:=t,0,tk; tk=6;
ПОКАЗАТЬ:=y(x),
 vx,vy,v,ax,ay,a,
 atau,an,ro;

РАСЧЕТ:=КИНЕМАТИКА;
{
РАБОТА:=№1/2 - Кинематика точки (пространственный случай);
z=t;
vz=z't; az=vz't;
v=sqrt(vx^2+vy^2+vz^2);                    #  скорость т.М #
a=sqrt(ax^2+ay^2+az^2);                    # ускорение т.М #
atau=(vx*ax+vy*ay+vz*az)/v; # тангенциальное ускорение т.М #

ПОКАЗАТЬ:=z(x,y),z(x),z(y);

РАСЧЕТ:=КИНЕМАТИКА;
}
КОНЕЦ;
